x=c(43.0,31.0,2.8,6.7,27.0,12.0,1.4,20.0,1.7,17.0,4.5,17.0,4.5,190.0,210.0,42.0,33.0,14.0,6.7,120.0,66.0,11.0)

shapiro.test(x) # p-value = 5.36e-06 : les données de "x" ne semblent pas échantillonnées à partir d'une distribution normale
shapiro.test(log(x)) # p-value = 0.8851 : les logarithmes des données de "x" semblent échantillonnés à partir d'une distribution normale
